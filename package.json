		"@vitejs/plugin-react": "^5.0.2",
		"concurrently": "~9.2.1",
		"prettier": "^3.6.2",
		"prettier-plugin-tailwindcss": "0.6.14",
		"tailwindcss": "^4.1.12",
		"typescript": "^5.9.2",
