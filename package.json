		"@fontsource-variable/quicksand": "^5.2.8",
		"@iconify/react": "^6.0.0",
		"@tanstack/react-router": "^1.131.28",
		"@tanstack/react-router-with-query": "^1.130.17",
		"@tanstack/react-start": "^1.131.28",
		"arktype": "^2.1.20",
		"convex": "^1.26.2",
		"convex-helpers": "^0.1.104",
		"date-fns": "^4.1.0",
		"es-toolkit": "^1.39.10",
		"lru-cache": "^11.1.0",
		"react": "^19.1.1",
